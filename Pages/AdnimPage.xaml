<Page x:Class="UchPraktika.Pages.AdnimPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UchPraktika.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdnimPage">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button x:Name="RequestBTN" Content="Журнал заявок" HorizontalAlignment="Center" VerticalAlignment="Center" Click="RequestBTN_Click" FontWeight="Bold" Margin="0,0,0,10"/>
        <Button x:Name="UserBTN" Content="Список пользователенй" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Margin="0,0,0,10" Click="UserBTN_Click"/>
        <Button Content="Роли пользователей" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" Click="Button_Click"/>
        </StackPanel>

    </Grid>
</Page>
